<h1>キャンプアイテム編集</h1>
<%= form_with model: @camp_item do |f| %>

  <h4>アイテム画像</h4>
  <%= f.file_field :camp_item_image, accept: "image/*" %>

  <h4>キャンプアイテム</h4>
  <%= f.text_field :name %>

  <h4>感想</h4>
  <%= f.text_area :impression %>

  <%= f.submit "更新" %>


<% end %>
<td><%= link_to "戻る", camp_item_path(@camp_item.id) %></td>